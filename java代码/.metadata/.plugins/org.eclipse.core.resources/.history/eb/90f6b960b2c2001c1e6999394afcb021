package 第二周代码;

public class 代码05数组逆序输出 {
public static void main(String[] args) {
	int [] arr= {2,3,4,5,6};
	int i;
	System.out.println("逆序前的数组为：");
	for(i=0;i<arr.length;i++) {
		System.out.print(arr[i]);
    }
	for(i=0;i<arr.length;i++) {
		arr[i]=arr[arr.length-i];
	}
	System.out.println("逆序之后的数组为：");
	for(i=0;i<arr.length;i++) {
		System.out.print(arr[i]);
}
}

